<Application
    x:Class="Teretana.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/BlueTheme.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Themes/DarkTheme.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Themes/LightTheme.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Languages/English.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Languages/Serbian.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
